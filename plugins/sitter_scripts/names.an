# Names for the Sitter Disk plugin

introducer=name
project=sitter
sub_project=scripts

[public]
log_path="log_path"
default_log_path="/var/log/sitter/scripts"
default_log_subfolder="sitter-output"
log_subfolder="log_subfolder"
output="sitter_scripts_output"
output_default="/var/lib/sitter/script-files"
path="sitter_scripts_path"
path_default="/usr/share/sitter/scripts"
starter="script_starter"
starter_default="/usr/sbin/sitter-script-starter"

# vim: syntax=dosini
